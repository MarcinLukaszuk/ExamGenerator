<h2>Validate Exam</h2>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<style>
    .fa {
        margin-left: 8px;
        margin-right: 2px;
    }
</style>
<script src="//code.jquery.com/jquery-1.11.0.min.js"></script>

<div class="form-horizontal">

    <div class="form-group">

        <span class="control-label col-md-2">ZipArchive</span>
        <div class="col-md-7">
            <input type="file" name="FileUpload" class="btn btn-primary" id="fileInput" onchange="fileLength(this)" accept=".zip" />
        </div>
    </div>
    <div class="form-group">
        <span class="control-label col-md-2"></span>
        <div class="col-md-7">
            <button class="btn btn-primary" onclick="validate()">
                Upload<i id="spinner" class="fa fa-spinner fa-spin" style="display: none;"></i>
            </button>
        </div>
    </div>

</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<script>
     var jquerryFileUploadHref = "@Url.Action("Validate")"
</script>
<script src="~/Script/exam_validate.js"></script>